{
    "files": [
        "auto-imports.d.ts",
        "components.d.ts"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        },
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.vitest.json"
        }
    ],
    "types": [
        "vite/client",
        "vue-i18n",
        "node"
    ],
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom"
        ]
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.tsx",
        "src/**/*.vue",
        "**/*.ts",
        "**/*.d.ts",
        "**/*.vue"
    ],
    "exclude": [
        "dist",
        "node_modules"
    ]
}